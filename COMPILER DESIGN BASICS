#include <stdio.h>
#include <ctype.h>
#include <string.h>

int isKeyword(char *word) {
    char keywords[10][10] = {"int", "float", "if", "else", "while", "return"};
    int i;
    for (i = 0; i < 6; i++) {
        if (strcmp(word, keywords[i]) == 0) return 1;
    }
    return 0;
}


int main() {
    char input[] = "int x = 10 + y;";
    char token[20];
    int i = 0, j = 0;

    printf("Lexical Analysis:\n");
    while (input[i] != '\0') {
        if (isalnum(input[i])) {
            token[j++] = input[i];
        } else {
            if (j != 0) {
                token[j] = '\0';
                if (isKeyword(token)) printf("%s : Keyword\n", token);
                else printf("%s : Identifier\n", token);
                j = 0;
            }
            if (ispunct(input[i]) && input[i] != ' ') {
                printf("%c : Operator/Symbol\n", input[i]);
            }
        }
        i++;
    }
    return 0;
}
